{
  "log": {
    "loglevel": "warning",
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log"
  },
  "inbounds": [
    {
      "tag": "reality-in",
      "listen": "127.0.0.1",
      "port": 1443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "<this_should_be_replaced_by_vless_uuid>",
            "flow": "xtls-rprx-vision"
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": 2024
          }
        ]
      },
      "streamSettings": {
        "network": "raw",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "8443",
          "xver": 1,
          "serverNames": ["<this_should_be_replaced_by_root_domain>"],
          "privateKey": "<this_should_be_replaced_by_reality_pri_key>",
          "shortIds": ["e4c59e0a8bc6f50e", "40985e9a8547c46c"]
        },
        "rawSettings": {
          "acceptProxyProtocol": true
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "quic"]
      }
    },
    {
      "tag": "xhttp-in",
      "listen": "127.0.0.1",
      "port": 2024,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "<this_should_be_replaced_by_vless_uuid>"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "xhttp",
        "xhttpSettings": {
          "path": "<this_should_be_replaced_by_xhttp_path>",
          "mode": "auto"
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "quic"]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "wireguard",
      "protocol": "wireguard",
      "settings": {
        "secretKey": "<this_should_be_replaced_by_warp_private_key>",
        "address": [
          "172.16.0.2/32",
          "2606:4700:110:818c:1ed3:6a71:3fdc:8ccb/128"
        ],
        "peers": [
          {
            "publicKey": "<this_should_be_replaced_by_warp_public_key>",
            "endpoint": "engage.cloudflareclient.com:2408"
          }
        ]
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom"
    },
    {
      "tag": "block",
      "protocol": "blackhole"
    }
  ]
}
