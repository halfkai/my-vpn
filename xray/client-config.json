{
  "log": {
    "access": "/opt/homebrew/var/log/xray/access.log",
    "error": "/opt/homebrew/var/log/xray/error.log",
    "loglevel": "error"
  },
  "dns": {
    "servers": [
      {
        "address": "https+local://1.1.1.1/dns-query",
        "domains": ["geosite:geolocation-!cn"]
      },
      {
        "address": "https+local://8.8.8.8/dns-query",
        "domains": ["geosite:geolocation-!cn"]
      },
      {
        "address": "https+local://223.5.5.5/dns-query",
        "domains": ["geosite:cn"],
        "expectIPs": ["geoip:cn"]
      },
      {
        "address": "https+local://114.114.114.114/dns-query",
        "domains": ["geosite:cn"]
      },
      {
        "address": "https+local://[2606:4700:4700::1111]/dns-query",
        "domains": ["geosite:geolocation-!cn"]
      },
      {
        "address": "https+local://[2001:4860:4860::8888]/dns-query",
        "domains": ["geosite:geolocation-!cn"]
      },
      "localhost"
    ],
    "queryStrategy": "UseIP"
  },
  "inbounds": [
    {
      "tag": "socks-in",
      "protocol": "socks",
      "listen": "127.0.0.1",
      "port": 10800,
      "settings": {
        "udp": true
      }
    },
    {
      "tag": "http-in",
      "protocol": "http",
      "listen": "127.0.0.1",
      "port": 10801
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "<this_should_be_replaced_by_root_domain>",
            "port": 443,
            "users": [
              {
                "id": "<this_should_be_replaced_by_vless_uuid>",
                "flow": "xtls-rprx-vision",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "raw",
        "security": "reality",
        "realitySettings": {
          "fingerprint": "chrome",
          "serverName": "<this_should_be_replaced_by_root_domain>",
          "publicKey": "<this_should_be_replaced_by_reality_pub_key>",
          "path": "<this_should_be_replaced_by_xhttp_path>",
          "shortId": "e4c59e0a8bc6f50e"
        },
        "tlsSettings": {
          "serverName": "<this_should_be_replaced_by_root_domain>",
          "alpn": ["h3", "h2"]
        }
      }
    },
    {
      "tag": "proxy-xhttp",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "<this_should_be_replaced_by_root_domain>",
            "port": 443,
            "users": [
              {
                "id": "<this_should_be_replaced_by_vless_uuid>",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "xhttp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "<this_should_be_replaced_by_root_domain>",
          "fingerprint": "chrome",
          "alpn": ["h3", "h2"]
        },
        "xhttpSettings": {
          "path": "<this_should_be_replaced_by_xhttp_path>",
          "mode": "auto"
        }
      }
    },
    {
      "tag": "wireguard",
      "protocol": "wireguard",
      "settings": {
        "secretKey": "<this_should_be_replaced_by_warp_private_key>",
        "peers": [
          {
            "publicKey": "<this_should_be_replaced_by_warp_public_key>",
            "endpoint": "engage.cloudflareclient.com:2408"
          }
        ]
      },
      "streamSettings": {
        "sockopt": {
          "dialerProxy": "proxy-reality"
        }
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom"
    },
    {
      "tag": "block",
      "protocol": "blackhole"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private", "geoip:cn"],
        "domain": [
          "domain:<this_should_be_replaced_by_root_domain>",
          "geosite:cn",
          "geosite:apple",
          "geosite:private",
          "geosite:icloud",
          "geosite:apple-cn",
          "geosite:google-cn",
          "geosite:tld-cn",
          "geosite:adobe",
          "geosite:category-games@cn",
          "domain:*.microsoft.com"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "protocol": ["bittorrent"],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "domain": [
          "domain:*.cursor.sh",
          "domain:*.instagram.com",
          "domain:*.cdninstagram.com",
          "domain:iplocation.com",
          "domain:iplocation.net"
        ],
        "outboundTag": "wireguard"
      },
      {
        "type": "field",
        "domain": ["geosite:category-ads-all"],
        "outboundTag": "block"
      },
      {
        "type": "field",
        "domain": ["geosite:geolocation-!cn"],
        "outboundTag": "proxy"
      }
    ]
  }
}
