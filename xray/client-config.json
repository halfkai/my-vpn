{
  "log": {
    "access": "/opt/homebrew/var/log/xray/access.log",
    "error": "/opt/homebrew/var/log/xray/error.log",
    "loglevel": "error"
  },
  "dns": {
    "servers": [
      {
        "address": "1.1.1.1",
        "domains": ["geosite:geolocation-!cn"]
      },
      {
        "address": "2606:4700:4700::1111",
        "domains": ["geosite:geolocation-!cn"]
      },
      {
        "address": "8.8.8.8",
        "domains": ["geosite:geolocation-!cn"]
      },
      {
        "address": "2001:4860:4860::8888",
        "domains": ["geosite:geolocation-!cn"]
      },
      {
        "address": "223.5.5.5",
        "domains": ["geosite:cn"],
        "expectIPs": ["geoip:cn"]
      },
      {
        "address": "2400:3200::1",
        "domains": ["geosite:cn"],
        "expectIPs": ["geoip:cn"]
      },
      {
        "address": "114.114.114.114",
        "domains": ["geosite:cn"]
      },
      "localhost"
    ],
    "queryStrategy": "UseIP"
  },
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "domain": ["geosite:category-ads-all"],
        "outboundTag": "block"
      },
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "block"
      },
      {
        "type": "field",
        "domain": [
          "geosite:cn",
          "geosite:private",
          "geosite:apple-cn",
          "geosite:google-cn",
          "geosite:tld-cn",
          "geosite:category-games@cn"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "ip": [
          "geoip:cn",
          "<this_should_be_replaced_by_my_server_ipv4>",
          "<this_should_be_replaced_by_my_server_ipv6>"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "domain": [
          "domain:www.instagram.com",
          "domain:instagram.com",
          "domain:iplocation.com",
          "domain:iplocation.net"
        ],
        "outboundTag": "wireguard"
      },
      {
        "type": "field",
        "domain": ["geosite:geolocation-!cn"],
        "outboundTag": "proxy"
      }
    ]
  },
  "inbounds": [
    {
      "tag": "socks-in",
      "protocol": "socks",
      "listen": "127.0.0.1",
      "port": 10800,
      "settings": {
        "udp": true
      }
    },
    {
      "tag": "http-in",
      "protocol": "http",
      "listen": "127.0.0.1",
      "port": 10801
    }
  ],
  "outbounds": [
    {
      "tag": "wireguard",
      "protocol": "wireguard",
      "settings": {
        "secretKey": "<this_should_be_replaced_by_warp_private_key>",
        "peers": [
          {
            "publicKey": "<this_should_be_replaced_by_warp_public_key>",
            "endpoint": "engage.cloudflareclient.com:2408"
          }
        ]
      },
      "streamSettings": {
        "sockopt": {
          "dialerProxy": "proxy"
        }
      }
    },
    {
      "tag": "proxy",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "<this_should_be_replaced_by_root_domain>",
            "port": 443,
            "users": [
              {
                "id": "<this_should_be_replaced_by_vless_uuid>",
                "flow": "xtls-rprx-vision",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "fingerprint": "chrome",
          "serverName": "<this_should_be_replaced_by_root_domain>",
          "publicKey": "<this_should_be_replaced_by_reality_pub_key>",
          "path": "<this_should_be_replaced_by_xhttp_path>",
          "shortId": "e4c59e0a8bc6f50e"
        },
        "tlsSettings": {
          "serverName": "<this_should_be_replaced_by_root_domain>",
          "alpn": ["h3"]
        }
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom"
    },
    {
      "tag": "block",
      "protocol": "blackhole"
    }
  ]
}
